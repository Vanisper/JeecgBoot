{
  "name": "jeecgboot-vue3",
  "version": "0.0.0",
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prepare": "npx lefthook install",
    "commit": "git-cz",
    "lint": "eslint .",
    "lint:flx": "eslint . --fix",
    "release:pre": "tsx scripts/release/index.ts",
    "release:commit": "tsx scripts/release/commit.ts",
    "release": "npm-run-all --serial release:pre release:commit",
    "changelog": "tsx scripts/changelog/index.ts",
    "clean:lib": "rimraf node_modules",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "dev": "cross-env VITE_CJS_TRACE=true vite",
    "build": "cross-env NODE_ENV=production NODE_OPTIONS=--max-old-space-size=8192 vite build && tsx ./build/script/postBuild.ts",
    "build:report": "pnpm clean:cache && cross-env REPORT=true npm run build",
    "preview": "npm run build && vite preview",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf yarn.lock && rimraf package.lock.json && rimraf node_modules && npm run install",
    "gen:icon": "tsx ./build/generate/icon/index.ts",
    "batch:prettier": "prettier --write  \"src/**/*.{js,ts,json,tsx,css,less,scss,vue,html,md}\""
  },
  "dependencies": {
